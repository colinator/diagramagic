<diag:diagram xmlns="http://www.w3.org/2000/svg" xmlns:diag="https://diagramagic.ai/ns" diag:padding="16">
  <style>
    .card { fill:#fff7ed; stroke:#9a3412; stroke-width:1; }
    .title { font-size:13px; }
  </style>
  <diag:flex id="top" x="90" y="20" width="150" padding="10" background-class="card"><text class="title">Top</text></diag:flex>
  <diag:flex id="bottom" x="90" y="240" width="150" padding="10" background-class="card"><text class="title">Bottom</text></diag:flex>
  <diag:arrow from="top" to="bottom" stroke="#c2410c" stroke-width="2"/>
</diag:diagram>
